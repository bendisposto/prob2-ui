<?xml version="1.0" encoding="UTF-8"?>

<?import de.prob2.ui.consoles.b.BConsole?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>
<fx:root type="Stage" title="B Console" xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1">
	<scene>
		<Scene stylesheets="@/prob.css" width="800" height="600">
			<!-- GroovyConsole does not extend Pane, so we have to wrap it in a StackPane so we can add the menu bar to it on Mac. -->
			<StackPane>
				<BConsole fx:id="bConsole" prefHeight="512.0" prefWidth="1024.0" />
			</StackPane>
		</Scene>
	</scene>
</fx:root>
