<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CustomMenuItem?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<fx:root type="TitledPane" minHeight="200.0"
	minWidth="200.0" text="Operations" xmlns="http://javafx.com/javafx/8.0.65"
	xmlns:fx="http://javafx.com/fxml/1">
	<content>
		<AnchorPane minHeight="100.0" minWidth="200.0" prefHeight="500.0"
			prefWidth="300.0">
			<children>
				<VBox prefHeight="200.0" prefWidth="100.0"
					AnchorPane.bottomAnchor="2.0" AnchorPane.leftAnchor="2.0"
					AnchorPane.rightAnchor="2.0" AnchorPane.topAnchor="2.0">
					<children>
						<AnchorPane prefHeight="35.0" prefWidth="246.0">
							<children>
								<Button fx:id="searchButton" alignment="CENTER"
										contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false"
										onAction="#handleSearchButton" prefWidth="35.0"
										stylesheets="@../../../../prob.css" AnchorPane.bottomAnchor="0.0"
										AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
									<opaqueInsets>
										<Insets />
									</opaqueInsets>
								</Button>
								<TextField fx:id="filterEvents" onAction="#handleSearchButton"
									prefWidth="217.0" promptText="Filter Events"
									AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="36.0"
									AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
							</children>
						</AnchorPane>
						<AnchorPane prefHeight="27.0" prefWidth="246.0">
							<children>
								<Button fx:id="backButton" layoutX="60.0" layoutY="1.0"
										mnemonicParsing="false" onAction="#handleBackButton" prefWidth="25.0"
										stylesheets="@../../../../prob.css" AnchorPane.bottomAnchor="0.0"
										AnchorPane.leftAnchor="51.0" AnchorPane.topAnchor="0.0" />
								<Button fx:id="forwardButton" layoutX="86.0" layoutY="1.0"
									mnemonicParsing="false" onAction="#handleForwardButton"
									prefWidth="25.0" stylesheets="@../../../../prob.css"
									AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="76.0"
									AnchorPane.topAnchor="0.0" />
								<Button fx:id="sortButton" layoutX="106.0" layoutY="21.0"
									mnemonicParsing="false" onAction="#handleSortButton" prefWidth="30.0"
									styleClass="aToZ" stylesheets="@../../../../prob.css"
									AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="30.0"
									AnchorPane.topAnchor="0.0" />
								<ToggleButton fx:id="disabledOpsToggle" layoutX="123.0"
									layoutY="1.0" mnemonicParsing="false" onAction="#handleDisabledOpsToggle"
									prefWidth="30.0" styleClass="eyeopen" stylesheets="@../../../../prob.css"
									AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0"
									AnchorPane.topAnchor="0.0" />
								<MenuButton fx:id="randomButton" layoutX="34.0"
											layoutY="1.0" mnemonicParsing="false" prefWidth="50.0"
											stylesheets="@../../../../prob.css" AnchorPane.bottomAnchor="0.0"
											AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
									<items>
										<MenuItem fx:id="oneRandomEvent" mnemonicParsing="false"
											onAction="#random" text="Execute 1 Event" />
										<MenuItem fx:id="fiveRandomEvents" mnemonicParsing="false"
											onAction="#random" text="Execute 5 Events" />
										<MenuItem fx:id="tenRandomEvents" mnemonicParsing="false"
											onAction="#random" text="Execute 10 Events" />
										<CustomMenuItem mnemonicParsing="false"
											text="Unspecified Action">
											<content>
												<AnchorPane>
													<children>
														<TextField fx:id="randomText" onAction="#random"
															promptText="# of Events" AnchorPane.rightAnchor="40.0" />
														<Button fx:id="randomEventsButton" maxWidth="40.0"
															mnemonicParsing="false" onAction="#random" prefWidth="40.0"
															text="Go!" AnchorPane.rightAnchor="0.0" />
													</children>
												</AnchorPane>
											</content>
										</CustomMenuItem>
									</items>
								</MenuButton>
							</children>
						</AnchorPane>
						<ListView fx:id="opsListView" prefHeight="673.0"
							prefWidth="296.0" />
					</children>
				</VBox>
			</children>
		</AnchorPane>
	</content>
</fx:root>
