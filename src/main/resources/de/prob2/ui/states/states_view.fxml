<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeItem?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<fx:root type="AnchorPane" maxHeight="Infinity" maxWidth="Infinity"
	minHeight="-Infinity" minWidth="-Infinity"
	xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1">
	<BorderPane AnchorPane.bottomAnchor="0.0"
		AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
		AnchorPane.topAnchor="0.0">
		<top>
			<HBox fillHeight="false" prefHeight="38.0" prefWidth="600.0"
				spacing="5">
				<Button fx:id="editBlacklistButton" alignment="CENTER_RIGHT"
					text="Edit Class Blacklist" />
			</HBox>
		</top>
		<center>
			<TreeTableView fx:id="tv" prefHeight="400.0"
				prefWidth="600.0">
				<columns>
					<TreeTableColumn fx:id="tvName" editable="false"
						prefWidth="200.0" sortable="false" text="Name" />
					<TreeTableColumn fx:id="tvValue" editable="false"
						prefWidth="200.0" sortable="false" text="Value" />
					<TreeTableColumn fx:id="tvPreviousValue" editable="false"
						prefWidth="200.0" sortable="false" text="Previous Value" />
				</columns>
				<TreeItem fx:id="tvChildrenItem" expanded="true" />
			</TreeTableView>
		</center>
	</BorderPane>
</fx:root>
