<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root minHeight="70.0" minWidth="200.0" stylesheets="@/prob.css"
	type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
	<TableView fx:id="animationsTable" AnchorPane.bottomAnchor="1.0"
		AnchorPane.leftAnchor="-1.0" AnchorPane.rightAnchor="-1.0"
		AnchorPane.topAnchor="0.0">
		<columns>
			<TableColumn fx:id="machine" minWidth="117.0" prefWidth="117.0"
				text="Machine/Context" />
			<TableColumn fx:id="lastop" minWidth="102.0" prefWidth="102.0"
				text="Last Operation" />
			<TableColumn fx:id="tracelength" minWidth="88.0"
				prefWidth="88.0" styleClass="center" text="Trace length" />
			<TableColumn fx:id="time" minWidth="65.0" prefWidth="65.0"
				text="Time" />
		</columns>
		<columnResizePolicy>
			<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
		</columnResizePolicy>
		<placeholder>
			<Label text="No model loaded" />
		</placeholder>
	</TableView>
</fx:root>
