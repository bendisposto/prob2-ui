<?xml version="1.0" encoding="UTF-8"?>

<?import de.prob2.ui.verifications.MachineTableView?>
<?import javafx.scene.control.*?>
<fx:root stylesheets="@/prob.css" minHeight="150" type="MachineTableView"
		 xmlns:fx="http://javafx.com/fxml/1">
	<columnResizePolicy>
		<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
	</columnResizePolicy>
	<columns>
		<TableColumn fx:id="machineStatusColumn" minWidth="80.0"
			maxWidth="80.0" prefWidth="80.0" text="%verifications.columns.status" />
		<TableColumn fx:id="machineNameColumn" minWidth="120.0"
			maxWidth="240.0" prefWidth="120.0" text="%project.machines.dialog.title" />
		<TableColumn fx:id="machineDescriptionColumn" minWidth="120.0"
			prefWidth="120.0" text="%verifications.columns.description" />
	</columns>
	<placeholder>
		<Label text="%project.machines.tab.noMachines" />
	</placeholder>
</fx:root>	
