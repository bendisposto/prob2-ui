<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<fx:root type="AnchorPane" xmlns:fx="http://javafx.com/fxml/1">
	<VBox spacing="8.0" AnchorPane.bottomAnchor="10.0"
		AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
		AnchorPane.topAnchor="0.0">
		<AnchorPane fx:id="resultBackground" styleClass="mcheckNoCheck"
			stylesheets="@/prob.css">
			<Text fx:id="resultText" fill="#8a8a8a" strokeType="OUTSIDE"
				strokeWidth="0.0" text="%verifications.modelchecking.stats.placeholder"
				AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0"
				AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0" id="bold" />
		</AnchorPane>
		<AnchorPane>
			<VBox fx:id="statsBox" spacing="13.0" visible="false"
				AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
				AnchorPane.topAnchor="5.0" AnchorPane.bottomAnchor="5.0">
				<HBox prefHeight="12.0" spacing="7.0">
					<Label text="%verifications.modelchecking.stats.elapsedTime" id="bold" />
					<Label fx:id="elapsedTime" />
				</HBox>
				<VBox spacing="3.0">
					<HBox prefHeight="12.0" spacing="7.0">
						<Label text="%verifications.modelchecking.stats.processedStates"
							id="bold" />
						<Label fx:id="processedStates" />
					</HBox>
					<HBox prefHeight="12.0" spacing="7.0">
						<Label text="%verifications.modelchecking.stats.totalStates"
							id="bold" />
						<Label fx:id="totalStates" />
					</HBox>
					<HBox prefHeight="12.0" spacing="7.0">
						<Label text="%verifications.modelchecking.stats.totalTransitions"
							id="bold" />
						<Label fx:id="totalTransitions" />
					</HBox>
				</VBox>
			</VBox>
		</AnchorPane>
	</VBox>
</fx:root>
