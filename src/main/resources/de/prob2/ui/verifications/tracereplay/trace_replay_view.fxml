<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import de.prob2.ui.helpsystem.HelpButton?>

<fx:root type="ScrollPane" fitToHeight="true" fitToWidth="true"
	stylesheets="@/prob.css" xmlns:fx="http://javafx.com/fxml/1">
	<AnchorPane>
		<VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="6.0" spacing="-4">
			<HBox alignment="CENTER_RIGHT">
				<AnchorPane>
					<HelpButton fx:id="helpButton" AnchorPane.rightAnchor="-3.0" />
				</AnchorPane>
			</HBox>
			<TableView fx:id="traceTableView" VBox.vgrow="ALWAYS">
				<columnResizePolicy>
					<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
				</columnResizePolicy>
				<columns>
					<TableColumn fx:id="statusColumn" prefWidth="80.0" text="%verifications.columns.status" />
					<TableColumn fx:id="nameColumn" text="%verifications.columns.name" />
				</columns>
				<placeholder>
					<Label text="%verifications.tracereplay.placeholder" />
				</placeholder>
			</TableView>
		</VBox>
	</AnchorPane>
</fx:root>

