<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.65"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox spacing="8.0" AnchorPane.bottomAnchor="10.0"
			AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
			AnchorPane.topAnchor="5.0">
			<children>
				<AnchorPane fx:id="resultBackground" styleClass="mcheckNoCheck"
					stylesheets="@../../../../prob.css">
					<children>
						<Text fx:id="resultText" fill="#8a8a8a" strokeType="OUTSIDE"
							strokeWidth="0.0" text="No Model Checking Job done."
							AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0"
							AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0">
							<font>
								<Font name="System Bold" size="13.0" />
							</font>
						</Text>
					</children>
				</AnchorPane>
				<VBox fx:id="statsBox" spacing="13.0" visible="false">
					<children>
						<!--AnchorPane style="-fx-background-color: lightgrey;"> <children> 
							<HBox layoutX="4.0" layoutY="2.0" spacing="2.0" AnchorPane.bottomAnchor="2.0" 
							AnchorPane.leftAnchor="4.0" AnchorPane.rightAnchor="4.0" AnchorPane.topAnchor="2.0"> 
							<children> <ImageView fx:id="titelImage" fitHeight="15.0" fitWidth="15.0" 
							pickOnBounds="true" preserveRatio="true" /> <Text fx:id="titelText" strokeType="OUTSIDE" 
							strokeWidth="0.0" wrappingWidth="230.0" /> </children> </HBox> </children> 
							</AnchorPane -->
						<HBox minWidth="100.0" prefHeight="12.0" spacing="7.0">
							<children>
								<Label text="Time Elapsed:" />
								<Label fx:id="elapsedTime" />
							</children>
						</HBox>
						<VBox spacing="3.0">
							<children>
								<HBox minWidth="120.0" prefHeight="12.0" spacing="7.0">
									<children>
										<Label text="Processed Nodes:" />
										<Label fx:id="processedNodes" />
									</children>
								</HBox>
								<HBox minWidth="90.0" prefHeight="12.0" spacing="7.0">
									<children>
										<Label text="Total Nodes:" />
										<Label fx:id="totalNodes" />
									</children>
								</HBox>
								<HBox minWidth="110.0" prefHeight="12.0" spacing="7.0">
									<children>
										<Label text="Total Transitions:" />
										<Label fx:id="totalTransitions" />
									</children>
								</HBox>
							</children>
						</VBox>
						<AnchorPane>
							<children>
								<Label alignment="CENTER" prefHeight="16.0" text="Node Stats"
									AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
									AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="5.0">
									<font>
										<Font name="System Bold" size="15.0" />
									</font>
								</Label>
							</children>
						</AnchorPane>
						<VBox>
							<children>
								<GridPane>
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES"
											minWidth="160.0" prefWidth="160.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="120.0" prefWidth="120.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints minHeight="20.0" prefHeight="20.0"
											vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<AnchorPane
											style="-fx-border-color: transparent transparent grey transparent; -fx-border-width: 2;">
											<children>
												<Label minWidth="44.0" text="Status"
													AnchorPane.leftAnchor="10.0">
													<font>
														<Font name="System Bold" size="14.0" />
													</font>
												</Label>
											</children>
										</AnchorPane>
										<AnchorPane
											style="-fx-border-color: transparent transparent grey transparent; -fx-border-width: 2;"
											GridPane.columnIndex="1">
											<children>
												<Label minWidth="73.0" text="# of Nodes"
													AnchorPane.rightAnchor="10.0">
													<font>
														<Font name="System Bold" size="14.0" />
													</font>
												</Label>
											</children>
										</AnchorPane>
									</children>
								</GridPane>
								<GridPane fx:id="nodeStats">
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES"
											minWidth="120.0" prefWidth="120.0" />
										<ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES"
											maxWidth="120.0" prefWidth="120.0" />
									</columnConstraints>
								</GridPane>
							</children>
						</VBox>
						<AnchorPane>
							<children>
								<Label alignment="CENTER" prefHeight="16.0" text="Transitions Stats"
									AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
									AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="5.0">
									<font>
										<Font name="System Bold" size="15.0" />
									</font>
								</Label>
							</children>
						</AnchorPane>
						<VBox>
							<children>
								<GridPane>
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES"
											minWidth="120.0" prefWidth="140.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											prefWidth="140.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints minHeight="-Infinity"
											prefHeight="20.0" vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<AnchorPane prefHeight="18.0"
											style="-fx-border-color: transparent transparent grey transparent; -fx-border-width: 2;">
											<children>
												<Label minWidth="112.0" text="Transition Name"
													AnchorPane.leftAnchor="10.0">
													<font>
														<Font name="System Bold" size="14.0" />
													</font>
												</Label>
											</children>
										</AnchorPane>
										<AnchorPane prefHeight="18.0"
											style="-fx-border-color: transparent transparent grey transparent; -fx-border-width: 2;"
											GridPane.columnIndex="1">
											<children>
												<Label minWidth="105.0" text="# of Transitions"
													AnchorPane.rightAnchor="10.0">
													<font>
														<Font name="System Bold" size="14.0" />
													</font>
												</Label>
											</children>
										</AnchorPane>
									</children>
								</GridPane>
								<GridPane fx:id="transStats">
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES"
											minWidth="140.0" prefWidth="140.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="140.0" prefWidth="140.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints />
									</rowConstraints>
								</GridPane>
							</children>
						</VBox>
					</children>
				</VBox>
			</children>
		</VBox>
	</children>
</fx:root>
