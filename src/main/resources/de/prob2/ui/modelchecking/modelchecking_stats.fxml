                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="-Infinity" prefHeight="20.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <AnchorPane prefHeight="18.0" prefWidth="145.0" style="-fx-border-color: transparent transparent grey transparent; -fx-border-width: 2;">
