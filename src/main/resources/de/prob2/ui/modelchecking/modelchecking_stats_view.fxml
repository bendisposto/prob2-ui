<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<fx:root text="Model Check Stats" type="TitledPane"
	xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
	<content>
		<ScrollPane fitToHeight="true" fitToWidth="true"
			hbarPolicy="NEVER" vbarPolicy="ALWAYS">
			<content>
				<AnchorPane prefHeight="500.0">
					<children>
						<VBox fx:id="statsBox" maxWidth="-Infinity" prefHeight="200.0"
							spacing="5.0" AnchorPane.bottomAnchor="0.0"
							AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
							AnchorPane.topAnchor="0.0">
							<children>
								<AnchorPane fx:id="statsPane" />
							</children>
						</VBox>
					</children>
				</AnchorPane>
			</content>
		</ScrollPane>
	</content>
</fx:root>
