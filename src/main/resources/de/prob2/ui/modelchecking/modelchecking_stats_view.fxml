<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<fx:root fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER"
	stylesheets="@../../../../prob.css" type="ScrollPane" vbarPolicy="ALWAYS"
	xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
	<VBox>
		<AnchorPane>
			<children>
				<VBox fx:id="historyBox" style="-fx-border-color: grey;"
					AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="5.0"
					AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="32.0" />
				<Button prefHeight="26.0" prefWidth="26.0" contentDisplay="GRAPHIC_ONLY"
					AnchorPane.topAnchor="0.0" AnchorPane.rightAnchor="-4.0"
					onAction="#addModelCheck">
					<graphic>
						<FontAwesomeIconView fill="#555560" glyphName="PLUS_CIRCLE"
							size="26" />
					</graphic>
				</Button>
			</children>
		</AnchorPane>
		<AnchorPane fx:id="statsPane" />
	</VBox>
</fx:root>
