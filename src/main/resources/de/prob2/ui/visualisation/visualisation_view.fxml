<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.GridPane?>

<fx:root type="AnchorPane" xmlns:fx="http://javafx.com/fxml/1">
	<StackPane fx:id="probLogoStackPane" AnchorPane.bottomAnchor="0.0"
		AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
		AnchorPane.topAnchor="0.0">
		<ImageView>
			<image>
				<Image url="prob_logo.gif" />
			</image>
		</ImageView>
	</StackPane>
	<GridPane fx:id="visualisationGridPane" AnchorPane.bottomAnchor="10.0"
		AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
		AnchorPane.topAnchor="10.0" alignment="CENTER"/>
</fx:root>

