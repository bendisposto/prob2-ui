<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.collections.*?>
<?import java.lang.String?>

<fx:root type="VBox" spacing="10"
	xmlns:fx="http://javafx.com/fxml/1">
	<ListView fx:id="listView" prefHeight="200" VBox.vgrow="NEVER"/>
	<TextArea promptText="{x| ...}" VBox.vgrow="ALWAYS"
		styleClass="text-area-with-border">
		<VBox.margin>
			<Insets left="5.0" right="5.0" />
		</VBox.margin>
	</TextArea>
	<FlowPane fx:id="flowPane" VBox.vgrow="NEVER">
		<ComboBox fx:id="lineTypeComboBox" focusTraversable="false">
			<items>
				<FXCollections fx:factory="observableArrayList">
					<String fx:value="" />
					<String fx:value="-fx-stroke-dash-array: 12 4 5 4;" />
					<String fx:value="-fx-stroke-dash-array: 2 2 2 2;" />
				</FXCollections>
			</items>
		</ComboBox>
		<Button text="linecolor" />
		<Button text="linethickness" />
		<VBox.margin>
			<Insets left="5.0" right="5.0" />
		</VBox.margin>
	</FlowPane>
	<padding>
		<Insets bottom="20.0" />
	</padding>
</fx:root>
